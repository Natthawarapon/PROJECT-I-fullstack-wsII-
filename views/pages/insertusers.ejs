<% include ../partials/header %>
<div class="jumbotron">
            <h1 class="display-3">Users Addnew</h1>
<form action ="/users/insertusers" method="post">        
<div class="form-group">
  <label for="">ID</label>
  <input type="number" name="users_id" id="" 
  class="form-control" value="" placeholder="" aria-describedby="helpId">
  <small id="helpId" class="text-muted">User ID</small>
</div>
<div class="form-group">
  <label for="">Email</label>
  <input type="text" name="email" id="" 
  class="form-control" placeholder="" value="" aria-describedby="helpId">
  <small id="helpId" class="text-muted">User Email</small>
</div>
<div class="form-group">
  <label for="">Password</label>
  <input type="number" name="password" id="" 
  class="form-control" placeholder="" value="" aria-describedby="helpId">
  <small id="helpId" class="text-muted">User Password</small>
</div>
<a name="" id="" class="btn btn-secondary" href="/users" role="button">Cancel</a>
<button type="submit" class="btn btn-success"  >Save</button>

</div> 


<div class="form-group">
  <label for="">Create At</label>
  <input type="text"
    class="form-control" value="<%= time %>" name="time" id="" aria-describedby="helpId" placeholder="" >
  <small id="helpId" class="form-text text-muted">create at</small>
</div>

</form>

<% include ../partials/footer %>